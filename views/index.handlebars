<div id="requestJobModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
    aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h3 id="myModalLabel">Modal header</h3>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
            </div>
            <div class="modal-body">
                <form id="myForm" method="post">

                    <label for="request-name">Please enter a name for your project</label>
                    <br>
                    <input type="text" id="request-name">
                    <br>
                    {{!-- Request Description --}}
                    <label for="request-description">Please enter a description of what you'd like
                        done</label>
                    <br>
                    {{!-- <input type="text" id="request-description" style="width: 100%; height: 200px"> --}}
                    <textarea rows="6" cols="35" id="request-description"></textarea>
                    <br>
                    {{!-- Requester Name --}}
                    <label for="requester-name">What is your name or the primary contact for this
                        project?</label>
                    <br>
                    <input type="text" id="requester-name">
                    <br>
                    {{!-- Requester Email --}}
                    <label for="requester-email">Please enter your e-mail address</label>
                    <br>
                    <input type="text" id="requester-email">
                    <br>
                    {{!-- Requester Phone --}}
                    <label for="requester-phone">Please enter your phone number</label>
                    <br>
                    <input type="text" id="requester-phone">
                    <br>
                    {{!-- Request budget --}}
                    <label for="request-budget">Please enter your budget for this project</label>
                    <br>
                    <select data-placeholder="" class="form-control user-selection text-center" id="request-budget">
                        <option value="$0-$1,000">$0 - $1,000</option>
                        <option value="$1,000-$2,500">$1,000 - $2,500</option>
                        <option value="$2,500-$5,000">$2,500 - $5,000</option>
                        <option value="$5,000-$7,500">$5,000 - $7,500</option>
                        <option value="$7,500-$10,000">$7,500 - $10,000</option>
                        <option value="$10,000+">$10,000+</option>
                    </select>
                    <br>
                    {{!-- Request Misc. --}}
                    <label for="request-misc">Please enter any additional comments or details you'd
                        like
                        us to know before reaching out</label>
                    <br>
                    {{!-- <input type="text" id="request-misc" style="width: 100%; height: 200px"> --}}
                    <textarea rows="6" cols="35" id="request-misc"></textarea>
                    <br>



                    <button id="myFormSubmit" class="btn" type="submit">Submit</button>
                </form>
            </div>
            <div class="modal-footer">
                <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
                <button class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>


























